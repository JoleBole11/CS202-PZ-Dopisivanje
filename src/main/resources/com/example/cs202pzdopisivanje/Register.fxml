<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cs202pzdopisivanje.Windows.RegisterController">
   <children>
      <VBox alignment="CENTER" layoutX="168.0" layoutY="15.0" prefHeight="370.0" prefWidth="264.0">
         <children>
            <Label text="Username" />
            <TextField promptText="Username...">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></TextField>
            <Label text="Password" />
            <PasswordField promptText="Password...">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></PasswordField>
            <HBox alignment="CENTER" prefHeight="44.0" prefWidth="264.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#OnCancelButtonClick" text="Cancel">
                     <HBox.margin>
                        <Insets right="50.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#OnRegisterButtonClick" text="Register" />
               </children>
            </HBox>
         </children></VBox>
   </children>
</AnchorPane>
