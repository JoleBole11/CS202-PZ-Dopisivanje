<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cs202pzdopisivanje.Controllers.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="168.0" layoutY="15.0" prefHeight="370.0" prefWidth="264.0">
         <children>
            <Label text="Username" />
            <TextField promptText="Your username...">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></TextField>
            <Label text="Password" />
            <PasswordField promptText="Your password...">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </PasswordField>
            <Button mnemonicParsing="false" onAction="#OnLoginButtonClick" text="Login">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#OnRegisterButtonClick" text="Register" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
